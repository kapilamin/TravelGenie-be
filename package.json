{
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.2.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "appwrite": "^14.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node": "^22.2.0",
    "node-appwrite": "^12.0.1"
  }
}
